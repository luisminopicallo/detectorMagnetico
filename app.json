{
  "expo": {
    "name": "Detector MÃ¡gico",
    "slug": "detector-magico-mentalismo",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/images/image.png",
    "scheme": "detectormagnetico",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mentalismo.detectormagnetico",
      "userInterfaceStyle": "dark",
      "backgroundColor": "#000000",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "userInterfaceStyle": "dark",
      "backgroundColor": "#000000",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/image.png",
        "backgroundColor": "#000000"
      },
      "package": "com.mentalismo.detectormagnetico",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "statusBar": {
        "translucent": true,
        "backgroundColor": "#000000",
        "style": "light"
      },
      "navigationBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content",
        "visible": "true"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "overlay-swipe",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "248b822b-a5e8-4878-8cb5-27f50cf2520c"
      }
    }
  }
}